@ARTICLE{Shamay2007,
  author = {Shamay, Eric S. and Buch, Victoria and Parrinello, Michele and Richmond,
	Geraldine L.},
  title = {At the water's edge: Nitric acid as a weak acid},
  journal = {Journal of the American Chemical Society},
  year = {2007},
  volume = {129},
  pages = {12910--+},
  number = {43},
  month = oct,
  SN = {0002-7863},
  TC = {25},
  UT = {WOS:000250818900005},
  Z9 = {25},
  __markedentry = {[eric:6]},
  abstract = {Nitric acid plays a role in many important chemical processes that
	happen in our environment, often at surfaces where less is known
	about its reactive behavior. Recent studies have shown that undissociated
	nitric acid is present on the surface of a nitric acid solution.
	Using ab initio molecular dynamics simulations, we show that a nitric
	acid molecule present on an aqueous solution surface structures and
	orients in a way that significantly reduces its ability to be the
	strong dissociating acid that it is in aqueous solution. Hydrogen
	bonding to surface solvating water molecules plays a key role in
	this altered molecular behavior.},
  doi = {10.1021/ja074811f},
  owner = {eric},
  timestamp = {2011.12.26}
}

